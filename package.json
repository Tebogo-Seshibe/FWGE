{
  "name": "fwge",
  "version": "1.0.0",
  "description": "",
  "main": "app/app.js",
  "directories": {
    "test": "test"
  },
  "nodemonConfig": {
    "watch": [
      "src/",
      "app/"
    ],
    "ext": "ts",
    "verbose": true
  },
  "scripts": {
    "build": "npm run build:fwge && npm run build:app",
    "build:fwge": "browserify ./src/index.ts -p [ tsify --noImplicitAny --target ES6 ] > ./build/fwge.js",
    "build:app": "browserify ./app/scripts/index.ts -p [ tsify --noImplicitAny --target ES6 ] > ./build/script.js",
    "start": "node ./app/app.js",
    "test": "mocha --require ts-node/register ./test/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tebogo-Seshibe/FWGE.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Tebogo-Seshibe/FWGE/issues"
  },
  "homepage": "https://github.com/Tebogo-Seshibe/FWGE#readme",
  "dependencies": {
    "systemjs": "^3.0.1",
    "tsify": "^4.0.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/express": "^4.16.1",
    "@types/jsdom": "^12.2.3",
    "@types/mocha": "^5.2.6",
    "browserify": "^16.2.3",
    "chai": "^4.2.0",
    "express": "^4.16.4",
    "jsdom": "^14.0.0",
    "mocha": "^6.0.2",
    "ts-node": "^8.0.3",
    "typescript": "^3.4.1"
  }
}
